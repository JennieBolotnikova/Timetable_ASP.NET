@model TimetableApp.Web.Models.DisciplineViewModel
@{ ViewBag.Title = ""; }

<h3>Are you sure you want to <strong>delete this discipline</strong>?</h3>

<div class="alert alert-secondary" role="alert">
    <p><strong>@Html.LabelFor(e => e.DisciplineNmae): </strong>@Model.DisciplineNmae</p>
</div>
<div>
    @Html.ActionLink("Delete", "ConfirmDelete", "Discipline", new { id = Model.DisciplineID, }, new { @class = "btn btn-outline-danger" })
</div>
<br />
<div>
    <a asp-action="Index">Назад</a>
</div>
@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
